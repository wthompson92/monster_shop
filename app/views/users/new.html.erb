
<%= form_tag users_path do %>
	<%= label_tag :name %>
	<%= text_field_tag :name, params[:name] %>
<br>
	<%= label_tag :user_name %>
	<%= text_field_tag :user_name%>
<br>
	<%= label_tag :password %>
	<%= text_field_tag :password %>
<br>
	<%= label_tag :confirm_password %>
	<%= text_field_tag :password_confirmation %>
<br>
	<%= label_tag :address %>
  <%= text_field_tag :address, params[:address] %>
<br>

	<%= label_tag :city %>
  <%= text_field_tag :city, params[:city]%>
	<br>

	<%= label_tag :state %>
  <%= text_field_tag :state, params[:state] %>
	<br>

	<%= label_tag :zip%>
  <%= text_field_tag :zip, params[:zip]%>
	<br>

  <%= submit_tag 'Create Profile' %>
<% end %>
